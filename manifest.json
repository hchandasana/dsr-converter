{
  "manifest_version": 2,
  "name": "DSR Converter",
  "version": "1.0.1",
  "description": "Convert your E-mail DSR format into the Slack DSR format. -- Created by HHC",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.2.4.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}